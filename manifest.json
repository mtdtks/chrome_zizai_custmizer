{
    "manifest_version": 2,
    "name": "zizai_customizer",
    "description": "for zizai worker.",
    "version": "0.0.2.0",

    "permissions": [
    ],

    "icons": { "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png" },

    "background": {
        "scripts": ["js/background.js"]
    },

    "content_scripts": [
      {
        "matches": ["https://www.kosho.or.jp/koshoadmin/*"],
        "js": ["js/jquery-2.1.4.min.js","js/main.js","js/mask.js"]
      }
    ],
    "options_page": "option.html"
}
